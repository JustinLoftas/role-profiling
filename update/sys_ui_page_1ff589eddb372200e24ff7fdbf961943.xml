<?xml version="1.0" encoding="UTF-8"?>
<record_update table="sys_ui_page">
    <sys_ui_page action="INSERT_OR_UPDATE">
        <category>general</category>
        <client_script/>
        <description/>
        <direct>false</direct>
        <endpoint>x_78492_profiling_role_profile_comparison.do</endpoint>
        <html><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">

<g2:evaluate var="jvar_stamp">
  var gr = new GlideRecord('sys_ui_script');  
  gr.get('name', 'x_78492_profiling.Ng.profile.controller');
  gr.getValue('sys_updated_on');
</g2:evaluate> 
<link rel="stylesheet" type="text/css" href="a8b00b54db043200e24ff7fdbf961922.cssdbx"/>
<link rel="stylesheet" type="text/css" href="e37e4c1edb732200e24ff7fdbf961987.cssdbx"/>
<g:requires name="x_78492_profiling.angular.1.4.9.jsdbx" params="cache=$[jvar_stamp]"/>
<g:requires name="x_78492_profiling.Ng.profile.controller.jsdbx" params="cache=$[jvar_stamp]"/>
<g:requires name="x_78492_profiling.Ng.profile.directive.jsdbx" params="cache=$[jvar_stamp]"/>

	
<div ng-app="profileApp">
	<div ng-controller="ProfileController as profCtrl">
		<div class="container">
			   <div class="row">
				<div class="col-md-offset-2 col-md-4">
					<div class="panel panel-default panel-info">
						<div class="panel-heading column-row-text">${gs.getMessage('Choose a profile:')}</div>
						<div class="panel-body">
							<g:ui_reference id="profile" value="d92065e5db772200e24ff7fdbf96199c" displayvalue="ServiceNow Developer" ng-model="profCtrl.profileId" name="profile" table="x_78492_profiling_role_profile"/>
						</div>
					</div>
				</div>
				<div class="col-md-4">
					<div class="panel panel-default panel-info">
						<div class="panel-heading column-row-text ">${gs.getMessage('Choose a user:')}</div>
						<div class="panel-body">
							<g:ui_reference id="user" ng-model="profCtrl.userId" name="user" table="sys_user" query="active=true" value="21dd67cadb032200e24ff7fdbf9619dd" displayvalue="Justin Loftas"/>
						</div>
					</div>
				</div>
				<div class="col-md-2">
					<div class="btn-holder">
					<button class="btn btn-primary" type="button" ng-click="profCtrl.getData('profile', 'user');">${gs.getMessage('Compare')}</button>
					</div>
					<div class="btn-holder">
					<button class="btn btn-primary" type="button" ng-click="profCtrl.resetData()">${gs.getMessage('Clear')}</button>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-xs-12">
					<div ng-show="profCtrl.skills.length > 0">
						<div class="row">
							<div class="col-md-offset-2 col-md-4">
						        <div class="header-description green-text">${gs.getMessage('Skills match')}</div>
						        <div ng-mouseout="profCtrl.toggleShowMatchingDetails(false)"  ng-mouseover="profCtrl.toggleShowMatchingDetails(true)" class="canvas-container">
									<profile-gauge when-select="profCtrl.setSkillFilter(skill)" gauge-for="matched" gauge-percent="{{profCtrl.percentMatched}}" gauge-info="{color : 'lime', shadow_color : '#347220'}"></profile-gauge>
								</div>
					        </div>
							<div class="col-md-4">
								<div class="header-description red-text">${gs.getMessage('User skills missing')}</div>
								<div class="canvas-container"><profile-gauge when-select="profCtrl.setSkillFilter(skill)" gauge-for="missing" gauge-percent="{{profCtrl.percentMissing}}" gauge-info="{color: 'red', shadow_color : 'maroon'}"></profile-gauge></div>
							</div>
						</div>
						<div class="row">
							<div class="col-md-offset-2 col-md-4">
						        <div class="header-description blue-text">${gs.getMessage('User skills higher')}</div>
								<div class="canvas-container"><profile-gauge when-select="profCtrl.setSkillFilter(skill)" gauge-for="higher" gauge-percent="{{profCtrl.percentHigher}}" gauge-info="{color : '#24d6f2', shadow_color : '#137382'}"></profile-gauge></div>
					        </div>
							<div class="col-md-4">
								<div class="header-description orange-text">${gs.getMessage('User skills lower')}</div>
								<div class="canvas-container"><profile-gauge  when-select="profCtrl.setSkillFilter(skill)" gauge-for="lower" gauge-percent="{{profCtrl.percentLower}}" gauge-info="{color : 'orange', shadow_color : '#7f5a1e'}"></profile-gauge></div>
							</div>
						</div>
					</div>
				</div>
			</div>
            <div class="row">
				<div class="col-xs-12">
			
				</div>
			</div>
			<div class="row">
				<div class="col-xs-12" ng-show="profCtrl.skills.length == 0">
					<div class="column-row-test">${gs.getMessage('No skills')}</div>
				</div>
			</div>
			<div class="row">
				<div class="table-responsive" ng-show="profCtrl.skills.length > 0">
					<table class="table">
						<thead>
							<tr>
								<th>
									<div class="column-row-header">${gs.getMessage('Category')}</div>
								</th>
								<th>
									<div class="column-row-header">${gs.getMessage('Skill')}</div>
								</th>
							    <th>
									<div class="column-row-header">${gs.getMessage('Profile level')}</div>
								</th>
								<th>
									<div class="column-skill-row-header">${gs.getMessage('User skill level')}</div>
								</th>
							</tr>
						</thead>
						<tr ng-repeat="skills in profCtrl.skills | filter: {matched:profCtrl.levelFilter}">		
						    <td>
								<div class="column-row-text" ng-bind="skills.category"></div>
						    </td>
							<td>
								<div class="column-row-text" ng-bind="skills.skill"></div>
							</td>
							<td>
								<div class="column-row-text" ng-bind="skills.skill_level"></div>
							</td>
							<td class="column-skill-level">
								<div ng-show="skills.matched == 'missing'"  ng-class="profCtrl.getGlyphClass(skills.matched)">
								</div>
								<div ng-show="skills.matched != 'missing'" ng-bind="skills.user_level" ng-class="profCtrl.getGlyphClass(skills.matched)">
								</div>
							</td>
						</tr>
					</table>
				</div>
            </div>
		</div>
	</div>
</div>
</j:jelly>]]></html>
        <name>role_profile_comparison</name>
        <processing_script/>
