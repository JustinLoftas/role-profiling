        <name>update_sme</name>
        <operation_script><![CDATA[(function process(/*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {

	var data = request.body.data;
	var result = {
		outcome : 'error',
		message : 'Missing SME data'
	};
	if (!gs.nil(data)){
		result = new x_78492_profiling.ProfileManagementUtils().updateSME(data);
	}

	if (result.outcome == 'error'){
		var myError = new sn_ws_err.ServiceError();
		myError.setStatus(410);
		myError.setMessage('The SME value for the user could not be set. Error is: ' + result.message);
		return myError;
	}

	response.setBody(result);

})(request, response);]]></operation_script>
